{
    "functions": {
        "api/*.js": {
            "runtime": "@martinpham/vercel-runtime-playground@1.0.4",
            "excludeFiles": "{.cursor,.git,.pnpm-store,.vercel,node_modules,.env,docker,.next,tests,artisan,*.md,*.tsbuildinfo,*.yml}/**"
        }
    },
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        
    }
}
